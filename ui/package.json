{
  "name": "kvm-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "21.1.0"
  },
  "scripts": {
    "dev": "./dev_device.sh",
    "dev:ssl": "USE_SSL=true ./dev_device.sh",
    "dev:cloud": "vite dev --mode=cloud-development",
